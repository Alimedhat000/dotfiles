{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "arch",
    "type": "small",
    "padding": {
      "top": 1,
      "left": 3
    },
    "color": {
      "1": "{{ colors.primary.default.hex }}",
      "2": "{{ colors.secondary.default.hex }}"
    }
  },
  "display": {
    "color": {
      "separator": "{{ colors.tertiary.default.hex }}"
    },
    "separator": " ",
    "constants": ["   »»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»»"]
  },
  "modules": [
    "break",
    {
      "type": "kernel",
      "key": "   Distro   ",
      "keyColor": "{{ colors.primary.default.hex }}",
      "format": "{2}"
    },
    {
      "type": "uptime",
      "key": "   Uptime   ",
      "keyColor": "{{ colors.secondary.default.hex }}",
    },
    {
      "type": "shell",
      "key": "   Shell    ",
      "keyColor": "{{ colors.tertiary.default.hex }}",
      "format" : "{}"
    },
    {
      "type": "terminal",
      "key": "   Terminal ",
      "keyColor": "{{ colors.secondary.default.hex }}",
      "format": "{1}"
    },
    {
      "type": "cpu",
      "key": "   CPU      ",
      "keyColor": "{{ colors.primary.default.hex }}"

      ,"format": "{1}"
    },
    {
      "type": "gpu",
      "key": "   GPU      ",
      "keyColor": "{{ colors.tertiary.default.hex }}",
      "format": "{2}",
      "hideType": "integrated",
    },
    {
      "type": "memory",
      "key": "   RAM      ",
      "keyColor": "{{ colors.primary.default.hex }}",

    },
    {
      "type": "disk",
      "key": "   Disk     ",
      "keyColor": "{{ colors.secondary.default.hex }}"
    },
    {
      "type": "colors",
      "paddingLeft": 3,
      "symbol": "diamond"
    }
  ]
}
